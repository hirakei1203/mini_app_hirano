<%# <div class="contents row"> %>
<div class="content_post main-cards">
  <%= form_with(model: @log, url:"/logs/#{@log.id}" ,local: true) do |form| %>
    <h5 class="page_title">
      編集ページ
    </h5>
<div class="row">
 <div class="col s6 offset-s3">
      <div class="card #ef9a9a red lighten-3">
        <div class="card-content white-text">
          <span class="card-title"><%= form.text_field :date, placeholder: "日付", class:"new_log_form" %></span>
          <span class="card-title"><%= form.text_field :title, placeholder: "タイトル", class:"new_log_form" %></span>
          <span class="card-title"><%= form.text_area :logs, placeholder: "業務内容", class:"new_log_form" %></span>
          <p><%= form.text_area :problem, placeholder: "現在の具体的な課題" , rows: "10", class:"new_log_form" %></p> 
        </div>
       
      </div>
 
    <div class="submit">
    <button class="btn waves-effect waves-light submit_btn" type="submit" name="action">Submit
    <i class="material-icons right">send</i>
    </button>
    </div>
 </div>
  <% end %>
</div>


